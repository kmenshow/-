model Lab7_2
  // time [0:0.0005:0.1] 
  parameter Integer x0 = 22; // Начальное число знающих о товаре
  parameter Integer N = 3310; // максимальное число людей, которых может заинтересовать товар
  Real x(start=x0);
equation
  der(x) = (0.0000311 + 0.21*x)*(N-x);
  // Пик распространения приходится на момент, когда x = 1655, то есть половина от всех людей

end Lab7_2;
